## These options configure PWM to 100Hz 50% D
echo am33xx_pwm > /sys/devices/bone_capemgr.9/slots
echo bone_pwm_P9_42 > /sys/devices/bone_capemgr.9/slots
echo 0 > /sys/devices/ocp.3/pwm_test_P9_42.16/run
echo 1 > /sys/devices/ocp.3/pwm_test_P9_42.16/polarity
echo 5000000 > /sys/devices/ocp.3/pwm_test_P9_42.16/period
echo 4800000 > /sys/devices/ocp.3/pwm_test_P9_42.16/duty
echo 1 > /sys/devices/ocp.3/pwm_test_P9_42.16/run

